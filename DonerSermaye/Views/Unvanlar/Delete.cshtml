@model DonerSermaye.Models.Data.Unvanlar

@{
    ViewData["Title"] = "Ünvan Sil";
    ViewData["Baslik"] = "Ünvan Sil";
    Layout = "~/Views/Shared/doner.cshtml";
}


<h3>   @Html.DisplayFor(model => model.Unvan) ünvanını silmek istiyor musunuz?</h3>
<div>
 
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Ünvan:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Unvan)
        </dd>
    </dl>
    <hr />
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Sil" class="btn btn-danger" /> 
        <a asp-action="Index" class="btn btn-primary">Ünvan Listesi</a>
    </form>
</div>
